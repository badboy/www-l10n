{
    "locales": [{
            "smartling": "ms-MS",
            "application": "ms"
        }
    ],
    "resourceSets": [{
        "type": "FLUENT",
        "authorizeContent": "true",
        "pathRegex": "en\\/(?<path>.*)\\.ftl",
        "translationPathExpression": "${locale}/${path}.${originalFile.extension}",
        "translationCommitMessage": "Translate ${originalFile.fullName} to ${locale}"
    }]
}
